pipeline {

    agent any

    tools {
        nodejs '20.3.0'
    }

    stages {

        stage ('Compilar'){
            steps {
                echo 'Compilando ....'
                sh 'npm install'
            }
        }

        stage ('Test'){
            steps {
                echo 'Pasando las pruebas ....'
                sh 'npm test'
            }
        }
    }
}